<%= form_with model: @user, local: true do |f| %>
  <%= f.label "姓"%>
  <%= f.text_field :last_name %>
  <%= f.label "名"%>
  <%= f.text_field :first_name %>
  <%= f.label "メールアドレス"%>
  <%= f.text_field :email %>
  <%= f.label "勤務地"%>
  <%= f.text_field :workplace %>

  <%= f.submit "送信", class: 'btn btn-success' %>
<% end %>
